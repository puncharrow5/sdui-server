// 사이트
model Site {
    id     Int    @id @default(autoincrement())
    domain String @unique
    name   String
    email  String

    // 헤더
    header       Header?
    mobileHeader MobileHeader?

    // 컴포넌트
    compoents Component[]

    // 푸터
    footer Footer?

    // 관리자
    admins SiteAdmin[]
}
