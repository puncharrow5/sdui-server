// 컴포넌트 종류
enum ChildType {
    BOX
    IMAGE
}

// 자식 컴포넌트
model Child {
    id        Int       @id @default(autoincrement())
    childType ChildType
    title     String?
    content   String?

    // 스타일
    childStyle ChildStyle?

    // 삭제 여부
    isDelete Boolean @default(false)

    // 부모 컴포넌트
    componentId Int
    component   Component @relation(fields: [componentId], references: [id])
}
